{
  "name": "compassionatelawfulkernelmode",
  "module": "server.ts",
  "main": "./dist/server.js",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "NODE_ENV=development; bun run --port=8080 --smol --watch ./server.ts",
