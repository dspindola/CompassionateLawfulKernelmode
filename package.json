	"scripts": {
		"dev": "concurrently \"bun:server:dev\" \"bun:server:build\" \"bun:client:dev\"",
		"start": "NODE_ENV=production; bun run --port=8080 --smol ./dist/index.js",
		"server:debug": "bun --inspect --hot ./src/index.ts",
		"server:build": "bun build ./src/index.ts --define=process.env.NODE_ENV:\"'production'\" --sourcemap=external  --format=esm --target=bun --outdir=dist --minify",
		"server:start": "bun run ./dist/index.js",
		"server:dev": "NODE_ENV=development; bun run --port=8000 --smol --watch ./src/index.ts",
		"client:dev": "bun --bun vite --port=8001 --host"
	},
