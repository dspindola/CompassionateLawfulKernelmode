    "@elysiajs/static": "^1.0.3",
    "@elysiajs/stream": "^1.0.3",
    "@elysiajs/swagger": "^1.0.5",
    "@elysiajs/websocket": "^0.2.8",
    "@replit/database": "^3.0.1",
    "@replit/object-storage": "^1.0.0",
    "db0": "^0.1.4",
    "drizzle-kit": "^0.22.8",
    "drizzle-orm": "^0.31.4",
    "elysia": "^1.0.27",
    "logixlysia": "^3.4.0"
